<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bharatkumar.meritas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".AddStudentActivity"
            android:label="Add Student" />
        <activity
            android:name=".DeleteActivity"
            android:label="Delete Student" />
        <activity
            android:name=".AllDataActivity"
            android:label="All STUDENTS DETAILS" />
        <activity android:name=".UpdateActivity"></activity>
    </application>

</manifest>